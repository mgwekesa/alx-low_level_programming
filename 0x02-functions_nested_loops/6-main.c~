/*
 * the main function for evaluating the absolute value
 */

#include <stdio.h>

/**
 * main - Entry point for the program
 * Return: returns 0
 */

int main(void)
{
	int r;

	r = _abs(-1);
	printf("%d\n", r);
	r = _abs(0);
	printf("%d\n", r);
	r = _abs(1);
	printf("%d\n", r);
	r = _abs(-98);
	printf("%d\n", r);
	return (0);
}
